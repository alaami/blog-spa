{
    "name": "spa-blog-app",
    "version": 2,
    "builds": [
      {"src": "package.json","use": "@now/static-build"},
      { "src": "./services/ArticleService.ts", "use": "@now/node" }
    ],
    "routes": [
      {
        "src": "/(js|css|img)/.*",
        "headers": { "cache-control": "max-age=31536000, immutable" }
      },
      { "handle": "filesystem" },
      { "src": ".*", "dest": "/" }
    ],
    "env": {
      "VUE_APP_CLIENT_ID":"@client_id",
      "VUE_APP_API_MANAGER_URL":"@api_gateway"
    }
  }